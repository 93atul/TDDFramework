<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Google Search WebApp Suite" parallel="tests" verbose="1">
    <listeners>
        <listener class-name="common.utils.ListenerTransform"/>
        <listener class-name="common.utils.ExtentReportUtil" />
    </listeners>
    <parameter name="environment" value="QA"/>
    <parameter name="launchUrl" value="https://google.co.in"/>
    <test name="Google Search Tests" parallel="methods" thread-count="10">
        <classes>
            <class name="webapp.Google.GoogleSearchTests" />
        </classes>
    </test>
</suite>